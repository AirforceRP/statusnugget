{
  "name": "statusnugget",
  "version": "1.0.0",
  "description": "Status Uptime and Status Page monitoring application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm start",
    "dev:client:win": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "install:client": "cd client && npm install"
  },
  "keywords": [
    "status",
    "uptime",
    "monitoring",
    "status-page"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "express-rate-limit": "^7.1.5",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}

